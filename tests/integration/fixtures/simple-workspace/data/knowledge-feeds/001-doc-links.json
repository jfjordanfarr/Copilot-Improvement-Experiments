{
  "id": "simple-doc-links",
  "label": "Simple Workspace Documentation Links",
  "artifacts": [
    {
      "path": "docs/link-source.md",
      "layer": "requirements",
      "language": "markdown"
    },
    {
      "path": "docs/link-target.md",
      "layer": "requirements",
      "language": "markdown"
    },
    {
      "path": "src/core.ts",
      "layer": "code",
      "language": "typescript"
    }
  ],
  "links": [
    {
      "sourcePath": "docs/link-source.md",
      "targetPath": "docs/link-target.md",
      "kind": "documents"
    },
    {
      "sourcePath": "docs/link-target.md",
      "targetPath": "src/core.ts",
      "kind": "documents"
    }
  ]
}
