@page "/"
@model BlazorTelemetry.Pages.HostModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Blazor Telemetry Test</title>
    <script src="js/blazor-telemetry.js"></script>
</head>
<body>
    <div id="telemetry-endpoint"
         data-endpoint="@Model.TelemetryEndpoint"
         data-instrumentation="@Model.InstrumentationKey"></div>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>
</body>
</html>
